SECRET_KEY=your-secret-key-here
DEBUG=True
LOG_LEVEL=INFO
ALLOWED_HOSTS=localhost,127.0.0.1

# Database
DB_NAME=auto_tester
DB_USER=postgres
DB_PASSWORD=your-db-password
DB_HOST=localhost
DB_PORT=5432

# Redis
REDIS_URL=redis://localhost:6379/0

# Celery workers (run these in separate terminals):
# celery -A auto_tester worker -Q upload -l info -n upload@%h
# celery -A auto_tester worker -Q execution -l info -n execution@%h

# Docker Model Runner
DMR_HOST=localhost
DMR_PORT=12434
DMR_MODEL=ai/mistral
DMR_VISION_MODEL=ai/qwen3-vl
DMR_TEMPERATURE=0.9
DMR_MAX_TOKENS=16096
DMR_REQUEST_TIMEOUT=300

# OpenAI API (for vision model)
OPENAI_API_KEY=your-openai-api-key
OPENAI_BASE_URL=https://api.openai.com/v1/chat/completions
OPENAI_VISION_MODEL=gpt-4o
OPENAI_TEMPERATURE=0.1
OPENAI_MAX_TOKENS=4096
OPENAI_REQUEST_TIMEOUT=120

# Vision Backend: "dmr" or "openai"
VISION_BACKEND=dmr

# Agent
AGENT_MAX_ITERATIONS=30
AGENT_TIMEOUT_SECONDS=600

# Docker
DOCKER_HOST=unix:///var/run/docker.sock

# Testing Environment
ENV_IMAGE_NAME=auto-tester-env
ENV_IMAGE_TAG=latest
ENV_SSH_USER=root
ENV_SSH_PASSWORD=testpass123
ENV_VNC_PASSWORD=testpass123
ENV_CONTAINER_PREFIX=auto-tester-env
ENV_HEALTH_CHECK_TIMEOUT=60
ENV_HEALTH_CHECK_INTERVAL=2
ENV_SSH_TIMEOUT=10

# SSH Session
SSH_COMMAND_TIMEOUT=120
SSH_KEEPALIVE_INTERVAL=15

# Output Summarizer
DMR_SUMMARIZER_MODEL=ai/mistral
OUTPUT_SUMMARIZE_THRESHOLD=2000
OUTPUT_SUMMARIZE_CHUNK_SIZE=12000

# Context Summarizer
CONTEXT_SUMMARIZE_THRESHOLD=20000
CONTEXT_PRESERVE_LAST_MESSAGES=6
CONTEXT_SUMMARIZE_CHUNK_SIZE=8000

# OmniParser
OMNIPARSER_URL=http://localhost:8000
OMNIPARSER_API_KEY=your-omniparser-api-key
OMNIPARSER_REQUEST_TIMEOUT=600
OMNIPARSER_WEIGHTS_DIR=./OmniParser/weights
OMNIPARSER_BOX_THRESHOLD=0.05
OMNIPARSER_IOU_THRESHOLD=0.7
OMNIPARSER_CAPTION_BATCH_SIZE=64
